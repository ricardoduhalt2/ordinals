{
  "rewrites": [
    { "source": "/style.css", "destination": "/webapp/style.css" },
    { "source": "/script.js", "destination": "/webapp/script.js" },
    { "source": "/upload", "destination": "/webapp/server.js" },
    { "source": "/upload-image", "destination": "/webapp/server.js" },
    { "source": "/(.*)", "destination": "/webapp/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
